package p2023_7_13;

public class Oper02 {

	public static void main(String[] args) {
		
// 연결 연산자 : +		
		String str1 = "자바";
		String str2 = str1 + "오라클";
		System.out.println("str1 = " + str1);
		System.out.println("str2 = " + str2);
		
		String str3 = "파이썬";
		String str4 = "스프링";
		System.out.println(str3 + str4);
	}

}
