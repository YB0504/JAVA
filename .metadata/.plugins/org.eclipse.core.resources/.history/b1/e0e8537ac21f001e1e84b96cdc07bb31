		System.out.println("자바 출력 성공");

		System.out.println("Hello World");
		
		int score = 90;
		int score1;
		score1 = 95;
		System.out.println(score);
		
		int hour = 5;
		int minute = 03;
		
		System.out.println("PM " + hour + "시 " + minute + "분");
		
		int value = 30;
		int result = value + 10;
		System.out.println(result);
	}
