package p2023_7_13;

public class Oper11 {

	public static void main(String[] args) {
		
// 증감 연산자 예제 02
		
		int a= 10, b=10;
		
		System.out.println("a = " + a++); // 후행 연산
		System.out.println("a = " + a); // a = 11
		
		System.out.println("b = " + (++b)); // 후행 연산
		System.out.println("b = " + b); // b = 11
		
		
		
		
		
	}

}
